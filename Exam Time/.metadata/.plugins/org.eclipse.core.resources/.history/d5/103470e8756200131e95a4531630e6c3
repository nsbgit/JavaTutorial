package test;

public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = new int[7];
		
		System.out.println("array.length() = " + a.length);
		
		for(int i = 0; i< a.length; i++) {
			a[i] = 1+i;
		}
		
		System.out.println("array.length() = " + a.length);
		
		for(int i = 0; i< a.length; i++) {
			System.out.println("a[" + i + "] = " + a[i]);
		}

	}

}
